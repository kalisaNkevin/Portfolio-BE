  "bugs": {
    "url": "https://github.com/kalisaNkevin/Portfolio-BE/issues"
  },
  "homepage": "https://github.com/kalisaNkevin/Portfolio-BE#readme",
  "dependencies": {
    "c8": "^7.12.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "express": "^4.18.1",
    "mocha": "^10.0.0",
    "mongoose": "^6.6.0",
    "path": "^0.12.7",
