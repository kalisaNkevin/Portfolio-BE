    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.19.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-node": "^0.0.1-security",
    "babel-register": "^6.26.0",
    "nodemon": "^2.0.19"
